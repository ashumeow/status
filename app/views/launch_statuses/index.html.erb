<h1>Listing launch_statuses</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @launch_statuses.each do |launch_status| %>
  <tr>
    <td><%= launch_status.name %></td>
    <td><%= launch_status.description %></td>
    <td><%= link_to 'Show', launch_status %></td>
    <td><%= link_to 'Edit', edit_launch_status_path(launch_status) %></td>
    <td><%= link_to 'Destroy', launch_status, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Launch status', new_launch_status_path %>
